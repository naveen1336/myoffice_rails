<p id="notice"><%= notice %></p>

<h1>App Users</h1>

<table class="table">
  <thead>
    <tr>
      <th>Email</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Role</th>
      <th colspan="3">Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @app_users.each do |app_user| %>
      <tr>
        <td><%= app_user.email %></td>
        <td><%= app_user.first_name %></td>
        <td><%= app_user.last_name %></td>
        <td><%= app_user.role %></td>
        <td><%= link_to 'Show', app_user %></td>
        <td><%= link_to 'Edit', edit_app_user_path(app_user) %></td>
        <td><%= link_to 'Destroy', app_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New App User', new_app_user_path %>
